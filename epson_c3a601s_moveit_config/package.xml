<?xml version="1.0"?>
<package format="2">
  <name>epson_c3a601s_moveit_config</name>
  <version>0.1.0</version>
  <description>
    <p>
      MoveIt package for the Epson C3 A601S.
    </p>
    <p>
      An automatically generated package with all the configuration and launch
      files for using the Epson C3 A601S with the MoveIt Motion Planning
      Framework.
    </p>
  </description>
  <author>MoveIt Setup Assistant</author>
  <author>L. Castelli (U. Hasselt)</author>
  <author>W. vd. Aelst (U. Hasselt)</author>
  <maintainer email="g.a.vanderhoorn@tudelft.nl">G.A. vd. Hoorn (TU Delft Robotics Institute)</maintainer>

  <license>BSD</license>

  <url type="website">http://wiki.ros.org/epson_c3a601s_moveit_config</url>
  <url type="bugtracker">https://github.com/gavanderhoorn/epson_experimental/issues</url>
  <url type="repository">https://github.com/gavanderhoorn/epson_experimental</url>

  <buildtool_depend>catkin</buildtool_depend>

  <exec_depend>epson_c3_support</exec_depend>
  <exec_depend>industrial_robot_simulator</exec_depend>
  <exec_depend>joint_state_publisher</exec_depend>
  <exec_depend>moveit_fake_controller_manager</exec_depend>
  <exec_depend>moveit_planners_ompl</exec_depend>
  <exec_depend>moveit_ros_move_group</exec_depend>
  <exec_depend>moveit_ros_visualization</exec_depend>
  <exec_depend>moveit_ros_warehouse</exec_depend>
  <exec_depend>moveit_simple_controller_manager</exec_depend>
  <exec_depend>robot_state_publisher</exec_depend>
  <exec_depend>rviz</exec_depend>
  <exec_depend>warehouse_ros</exec_depend>
  <exec_depend>xacro</exec_depend>

  <export>
    <architecture_independent />
    <rosindex>
      <tags>
        <tag>c3</tag>
        <tag>epson</tag>
        <tag>experimental</tag>
        <tag>industrial</tag>
        <tag>moveit</tag>
        <tag>ros-industrial</tag>
      </tags>
    </rosindex>
  </export>
</package>
